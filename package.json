{
  "main": "./AppEntry.js",
  "version": "0.1.0",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "eslint": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "tsc": "tsc --noEmit",
    "test": "jest --watch --passWithNoTests",
    "test-ci": "cross-env CI=true jest --env=jsdom --passWithNoTests",
    "prettier": "prettier --ignore-path .eslintignore --parser json --write",
    "test-all": "yarn eslint && yarn test",
    "api-test": "wil-api-test api-test"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "yarn eslint",
      "git add"
    ],
    "{*.json,.{babelrc,eslintrc,prettierrc}}": [
      "prettier --ignore-path .eslintignore --parser json --write",
      "git add"
    ],
    "*.{test.ts,test.tsx}": [
      "yarn test-ci",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "tsc --noEmit && lint-staged"
    }
  },
  "jest": {
    "preset": "jest-expo",
    "timers": "fake",
    "testEnvironment": "jsdom",
    "transformIgnorePatterns": [
      "node_modules/(?!(jest-)?react-native|react-clone-referenced-element|@react-native-community|expo(nent)?|@expo(nent)?/.*|react-navigation|@react-navigation/.*|@unimodules/.*|sentry-expo|native-base)"
    ]
  },
  "dependencies": {
    "@expo/react-native-action-sheet": "^3.5.0",
    "@react-native-community/netinfo": "5.5.1",
    "axios": "^0.19.0",
    "expo": "^37.0.0",
    "expo-apple-authentication": "~2.1.0",
    "expo-blur": "~8.1.0",
    "expo-camera": "~8.2.0",
    "expo-constants": "~9.0.0",
    "expo-facebook": "~8.1.0",
    "expo-image-picker": "~8.1.0",
    "expo-localization": "~8.1.0",
    "expo-media-library": "~8.1.0",
    "he": "^1.2.0",
    "qs": "^6.9.1",
    "ramda": "^0.26.1",
    "react": "16.9.0",
    "react-dom": "16.9.0",
    "react-native": "https://github.com/expo/react-native/archive/sdk-37.0.1.tar.gz",
    "react-native-expo-image-cache": "^4.1.0",
    "react-native-gesture-handler": "~1.6.0",
    "react-native-paper": "^3.4.0",
    "react-native-pose": "^0.9.1",
    "react-native-reanimated": "~1.7.0",
    "react-native-render-html": "^4.1.2",
    "react-native-safe-area-context": "0.7.3",
    "react-native-screens": "~2.2.0",
    "react-native-syntax-highlighter": "^2.1.0",
    "react-native-version-check-expo": "^3.4.0",
    "react-native-web": "^0.11.7",
    "react-native-webview": "8.1.1",
    "react-navigation": "^4.0.10",
    "react-navigation-material-bottom-tabs": "^2.1.5",
    "react-navigation-slide-from-right-transition": "^1.0.4",
    "react-navigation-stack": "^1.10.3",
    "react-navigation-tabs": "^2.6.0",
    "react-redux": "^7.2.0",
    "react-redux-i18n": "^1.9.3",
    "reanimated-bottom-sheet": "^1.0.0-alpha.18",
    "redux": "^4.0.4",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "reselect": "^4.0.0"
  },
  "devDependencies": {
    "@babel/plugin-proposal-decorators": "^7.7.4",
    "@types/jest": "^24.0.23",
    "@types/qs": "^6.9.0",
    "@types/ramda": "^0.26.36",
    "@types/react": "^16.9.11",
    "@types/react-native": "^0.60.22",
    "@types/react-redux": "^7.1.7",
    "@types/react-redux-i18n": "^0.0.11",
    "@types/react-syntax-highlighter": "^11.0.2",
    "@types/react-test-renderer": "^16.9.1",
    "@types/redux": "^3.6.0",
    "@types/redux-logger": "^3.0.7",
    "@types/redux-persist": "^4.3.1",
    "@types/redux-saga": "^0.10.5",
    "@types/reselect": "^2.2.0",
    "@typescript-eslint/eslint-plugin": "^2.8.0",
    "@typescript-eslint/parser": "^2.8.0",
    "axios-mock-adapter": "^1.17.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "babel-preset-expo": "^8.1.0",
    "cross-env": "^6.0.3",
    "eslint": "^6.7.1",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "husky": "^3.1.0",
    "jest-expo": "^37.0.0",
    "lint-staged": "8.1.7",
    "prettier": "^1.19.1",
    "react-test-renderer": "16.8.3",
    "redux-logger": "^3.0.6",
    "redux-saga-test-plan": "^4.0.0-rc.3",
    "tslint": "^5.20.1",
    "tslint-import-rules": "^0.3.0",
    "tslint-lines-between-class-members": "^1.3.6",
    "typescript": "^3.8.3",
    "wil-api-test": "^1.2.2",
    "yarn": "^1.19.2"
  },
  "private": true
}
